{
  "rewrites": [
    {
      "source": "/old-route",
      "destination": "/new-route"
    }
  ],
  "redirects": [
    {
      "source": "/deprecated",
      "destination": "/new-path",
      "statusCode": 301
    }
  ],
  "headers": [
    {
      "source": "/some-path/*",
      "headers": [
        {
          "key": "X-Custom-Header",
          "value": "some-value"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
